{
    "name": "dpsoft/nova-persian-date",
    "description": "A Laravel Nova persian date field.",
    "keywords": [
        "laravel",
        "nova",
        "persian",
        "jalali",
        "shamsi",
        "dpsoft"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0 || >= 8.0"
    },
    "autoload": {
        "psr-4": {
            "Dpsoft\\NovaPersianDate\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dpsoft\\NovaPersianDate\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
